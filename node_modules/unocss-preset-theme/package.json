{
  "name": "unocss-preset-theme",
  "type": "module",
  "version": "0.12.0",
  "packageManager": "pnpm@8.11.0",
  "description": "dynamic theme",
  "author": "Dunqing <dengqing0821@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/Dunqing/unocss-preset-theme#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dunqing/unocss-preset-theme.git"
  },
  "bugs": {
    "url": "https://github.com/Dunqing/unocss-preset-theme/issues"
  },
  "keywords": [
    "unocss",
    "unocss-preset",
    "unocss-theme"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "*.css",
    "dist"
  ],
  "peerDependencies": {
    "@unocss/core": ">= 0.52.0"
  },
  "dependencies": {
    "@unocss/core": "^0.58.0",
    "@unocss/rule-utils": "^0.58.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.3.4",
    "@types/node": "^20.10.3",
    "@unocss/preset-uno": "^0.58.0",
    "bumpp": "^9.2.0",
    "eslint": "^8.55.0",
    "typescript": "^5.3.2",
    "unbuild": "^2.0.0",
    "vitest": "^1.0.1"
  },
  "scripts": {
    "build": "unbuild",
    "stub": "unbuild --stub",
    "play": "pnpm --filter ./playground run dev",
    "test": "vitest",
    "lint": "eslint .",
    "lint:fix": "pnpm run lint --fix",
    "typecheck": "tsc --noEmit",
    "release": "bumpp package.json packages/**/package.json"
  }
}